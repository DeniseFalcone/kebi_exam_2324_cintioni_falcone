        <description>no vegetarian, no allergies</description>
        <inputEntry id="UnaryTests_1ek84t0">
          <text>count(Vegetarian_Ingredients) = 1 and Vegetarian_Ingredients[1] = ""</text>
        </inputEntry>
        <inputEntry id="UnaryTests_1nz9tef">
          <text>count(Allergies_Ingredients) = 1 and Allergies_Ingredients[1] = ""</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_05duh4i">
          <text>Ingredients[list contains(Ingredients, item)]</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_0r4nnsw">
        <description>vegetarian, no allergies</description>
        <inputEntry id="UnaryTests_0dghjbt">
          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_0cjnwmt">
          <text>count(Allergies_Ingredients) = 1 and Allergies_Ingredients[1] = ""</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_164b4wy">
          <text>Ingredients[not (list contains(Vegetarian_Ingredients, item))]</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_0d5i837">
        <description>no vegetarian, allergies</description>
        <inputEntry id="UnaryTests_1hwlpam">
          <text>count(Vegetarian_Ingredients) = 1 and Vegetarian_Ingredients[1] = ""</text>
        </inputEntry>
        <inputEntry id="UnaryTests_1xn0s7j">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_1lm4iy1">
          <text>Ingredients[not (list contains(Allergies_Ingredients, item))]</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_1p1hwlw">
        <description>vegetarian, allergies</description>
        <inputEntry id="UnaryTests_192f45g">
          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_1o7r3fo">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_1icx1rt">
          <text>Ingredients[not (list contains(Vegetarian_Ingredients, item))][not (list contains(Allergies_Ingredients, item))]</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>

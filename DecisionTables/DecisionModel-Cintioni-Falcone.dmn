    <decisionTable id="DecisionTable_021xuf2" hitPolicy="FIRST">
      <input id="InputClause_18n61ey" label="Gluten_Ingredients" biodi:width="264" camunda:inputVariable="Gluten_Ingredients">
        <inputExpression id="LiteralExpression_15p1bow" typeRef="list">
          <text>Gluten_Ingredients</text>
        </inputExpression>
      </input>
      <input id="InputClause_02kothe" label="Lactose_Ingredients" biodi:width="287" camunda:inputVariable="Lactose_Ingredients">
        <inputExpression id="LiteralExpression_043dl2h" typeRef="list">
          <text>Lactose_Ingredients</text>
        </inputExpression>
      </input>
      <output id="OutputClause_1c5udck" label="Allergies_Ingredients" name="Allergies_Ingredients" typeRef="list" biodi:width="801" />
      <rule id="DecisionRule_0x0bhop">
        <description>no intolerances (non ritrono alcun ingrediente)</description>
        <inputEntry id="UnaryTests_1nitqbo">
          <text>count(Gluten_Ingredients) = 1 and Gluten_Ingredients[1] = ""</text>
        </inputEntry>
        <inputEntry id="UnaryTests_1iftijz">
          <text>count(Lactose_Ingredients) = 1 and Lactose_Ingredients[1] = ""</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_129lcze">
          <text>[""]</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_0quvlgd">
        <description>lactose intolerance (ritorno gli ingredienti con lattosio)</description>
        <inputEntry id="UnaryTests_15mxztc">
          <text>count(Gluten_Ingredients) = 1 and Gluten_Ingredients[1] = ""</text>
        </inputEntry>
        <inputEntry id="UnaryTests_0b2fikx">
          <text>count(Lactose_Ingredients) != 1</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_1s9yau0">
          <text>Lactose_Ingredients</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_1abft0q">
        <description>gluten intolerance (ritorno gli ingredienti con glutine)</description>
        <inputEntry id="UnaryTests_1dwc688">
          <text>count(Gluten_Ingredients) != 1</text>
        </inputEntry>
        <inputEntry id="UnaryTests_12rvni6">
          <text>count(Lactose_Ingredients) = 1 and Lactose_Ingredients[1] = ""</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_17xmhby">
          <text>Gluten_Ingredients</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_1wy7xgj">
        <description>all allergies (ritorno ingredienti con glutine e lattosio)</description>
        <inputEntry id="UnaryTests_1va8nye">
          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_1ny2sl9">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_004l6tv">
          <text>concatenate(Lactose_Ingredients, Gluten_Ingredients)</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
